{
    "name": "munkireport/munkireport-php",
    "description": "Reporting tool for munki",
    "type": "project",
    "license": "MIT",
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "haydenpierce/class-finder": "^0.4.2"
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.3",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "ext-dom": "*",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "hautelook/phpass": "^1.1",
        "rodneyrehm/plist": "^2.0",
        "defuse/php-encryption": "^2.1",
        "wikimedia/composer-merge-plugin": "dev-master",
        "munkireport/ard": "^3.0",
        "munkireport/reportdata": "^3.0",
        "munkireport/machine": "^6.0",
        "munkireport/event": "^3.0",
        "munkireport/disk_report": "^3.0",
        "munkireport/munkireport": "^3.0",
        "munkireport/applications": "^2.0",
        "munkireport/appusage": "^2.0",
        "munkireport/backup2go": "^1.0",
        "munkireport/bluetooth": "^2.0",
        "munkireport/caching": "^1.0",
        "munkireport/certificate": "^1.0",
        "munkireport/comment": "^4.0",
        "munkireport/crashplan": "^2.0",
        "munkireport/detectx": "^2.0",
        "munkireport/devtools": "^1.0",
        "munkireport/directory_service": "^1.0",
        "munkireport/displays_info": "^2.0",
        "munkireport/extensions": "^1.0",
        "munkireport/fan_temps": "^1.0",
        "munkireport/filevault_escrow": "^1.0",
        "munkireport/filevault_status": "^1.0",
        "munkireport/findmymac": "^1.0",
        "munkireport/firewall": "^1.0",
        "munkireport/firmware_escrow": "^1.0",
        "munkireport/fonts": "^1.0",
        "munkireport/gpu": "^1.0",
        "munkireport/homebrew": "^1.0",
        "munkireport/homebrew_info": "^1.0",
        "munkireport/ibridge": "^1.0",
        "munkireport/installhistory": "^2.0",
        "munkireport/inventory": "^4.0",
        "munkireport/laps": "^1.4",
        "munkireport/location": "^1.0",
        "munkireport/managedinstalls": "^2.0",
        "munkireport/mbbr_status": "^1.0",
        "munkireport/mdm_status": "^1.6",
        "munkireport/munki_facts": "^1.0",
        "munkireport/munkiinfo": "^1.0",
        "munkireport/munkireportinfo": "^1.0",
        "munkireport/network": "^3.0",
        "munkireport/network_shares": "^1.0",
        "munkireport/power": "^1.0",
        "munkireport/printer": "^1.0",
        "munkireport/profile": "^2.0",
        "munkireport/sccm_status": "^1.0",
        "munkireport/security": "^2.0",
        "munkireport/sentinelone": "^1.0",
        "munkireport/sentinelonequarantine": "^1.0",
        "munkireport/smart_stats": "^1.0",
        "munkireport/softwareupdate": "^1.0",
        "munkireport/sophos": "^1.0",
        "munkireport/supported_os": "^2.0",
        "munkireport/tag": "^3.0",
        "munkireport/timemachine": "^2.0",
        "munkireport/usage_stats": "^1.0",
        "munkireport/usb": "^1.0",
        "munkireport/user_sessions": "^1.0",
        "munkireport/warranty": "^4.0",
        "munkireport/wifi": "^1.0",
        "symfony/yaml": "^5.1",
        "adldap2/adldap2": "^10",
        "onelogin/php-saml": "^3.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/ui": "^2.0",
        "doctrine/dbal": "^2.10",
        "aacotroneo/laravel-saml2": "^2.1",
        "laravel/socialite": "^4.4",
        "adldap2/adldap2-laravel": "^6.1",
        "anhskohbo/no-captcha": "^3.2",
        "socialiteproviders/apple": "^3.0",
        "socialiteproviders/okta": "^1.1",
        "nuwave/lighthouse": "^4.15",
        "mll-lab/laravel-graphql-playground": "^2.1",
        "composer/composer": "^1.10",
        "pragmarx/firewall": "^2.3",
        "laravel/sanctum": "^2.4"
    },
    "autoload": {
        "classmap": [
            "app/Console/Commands/FakerDataStore.php",
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers/bootstrap_helper.php",
            "app/helpers/config_helper.php",
            "app/helpers/env_helper.php",
            "app/helpers/site_helper.php",
            "app/helpers/test_helper.php",
            "shims/Module_controller.php",
            "shims/View.php",
            "shims/Model.php",
            "shims/Capsule.php"
        ],
        "psr-4": {
            "munkireport\\models\\": "app/models/",
            "munkireport\\processors\\": "app/processors/",
            "munkireport\\builders\\": "database/builders/",
            "munkireport\\lib\\": "app/lib/munkireport",
            "Tests\\": "tests/",
            "App\\": "app/",
            "MR\\": "MR/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "composer.local.json"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php please package:discover --ansi",
            "MR\\Kiss\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php please key:generate --ansi"
        ]
    },
    "suggest": {
        "ext-ldap": "Needed to support Active Directory/LDAP Authentication"
    }
}
