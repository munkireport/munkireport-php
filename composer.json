{
    "name": "munkireport/munkireport-php",
    "description": "Reporting tool for munki",
    "type": "project",
    "license": "MIT",
    "version": "6.0.0",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.13.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "haydenpierce/class-finder": "^0.4.2",
        "phpstan/phpstan": "^0.12.82",
        "mll-lab/laravel-graphql-playground": "^2.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "ext-dom": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.5",
        "bordoni/phpass": "^0.3",
        "rodneyrehm/plist": "^2.0",
        "defuse/php-encryption": "^2.1",
        "munkireport/ard": "^3.0",
        "munkireport/event": "dev-master",
        "munkireport/disk_report": "dev-master",
        "munkireport/munkireport": "dev-master",
        "munkireport/applications": "dev-master",
        "munkireport/appusage": "dev-master",
        "munkireport/backup2go": "dev-master",
        "munkireport/bluetooth": "^2.0",
        "munkireport/caching": "dev-master",
        "munkireport/certificate": "dev-master",
        "munkireport/comment": "^4.0",
        "munkireport/crashplan": "dev-master",
        "munkireport/detectx": "dev-master",
        "munkireport/devtools": "^1.0",
        "munkireport/directory_service": "dev-master",
        "munkireport/displays_info": "dev-master",
        "munkireport/extensions": "dev-master",
        "munkireport/fan_temps": "dev-master",
        "munkireport/filevault_escrow": "^1.0",
        "munkireport/filevault_status": "dev-master",
        "munkireport/findmymac": "dev-master",
        "munkireport/firewall": "^1.0",
        "munkireport/firmware_escrow": "^1.0",
        "munkireport/fonts": "dev-master",
        "munkireport/gpu": "dev-master",
        "munkireport/homebrew": "dev-master",
        "munkireport/homebrew_info": "^1.0",
        "munkireport/ibridge": "^1.0",
        "munkireport/installhistory": "^2.0",
        "munkireport/inventory": "dev-master",
        "munkireport/laps": "^1.4",
        "munkireport/location": "^1.0",
        "munkireport/managedinstalls": "dev-master",
        "munkireport/mbbr_status": "^1.0",
        "munkireport/mdm_status": "^1.6",
        "munkireport/munki_facts": "^1.0",
        "munkireport/munkiinfo": "^1.0",
        "munkireport/munkireportinfo": "^1.0",
        "munkireport/network": "^3.0",
        "munkireport/network_shares": "^1.0",
        "munkireport/power": "dev-master",
        "munkireport/printer": "^1.0",
        "munkireport/profile": "^2.0",
        "munkireport/sccm_status": "^1.0",
        "munkireport/security": "dev-master",
        "munkireport/sentinelone": "dev-master",
        "munkireport/sentinelonequarantine": "^1.0",
        "munkireport/smart_stats": "^1.0",
        "munkireport/softwareupdate": "^1.0",
        "munkireport/sophos": "^1.0",
        "munkireport/supported_os": "^2.0",
        "munkireport/tag": "^3.0",
        "munkireport/timemachine": "^2.0",
        "munkireport/usage_stats": "^1.0",
        "munkireport/usb": "^1.0",
        "munkireport/user_sessions": "^1.0",
        "munkireport/warranty": "^4.0",
        "munkireport/wifi": "^1.0",
        "symfony/yaml": "^5.1",
        "adldap2/adldap2": "^10",
        "onelogin/php-saml": "^3.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/ui": "^3.0",
        "doctrine/dbal": "^2.10",
        "aacotroneo/laravel-saml2": "^2.1",
        "adldap2/adldap2-laravel": "^6.1",
        "anhskohbo/no-captcha": "^3.2",
        "nuwave/lighthouse": "^5.3.0",
        "composer/composer": "^2.0.13",
        "pragmarx/firewall": "^2.3",
        "laravel/sanctum": "^2.10",
        "xkerman/restricted-unserialize": "^1.1",
        "laravel/slack-notification-channel": "^2.3",
        "laravel-notification-channels/microsoft-teams": "^1.1",
        "wikimedia/composer-merge-plugin": "^2.0",
        "laravel/scout": "8.6",
        "teamtnt/laravel-scout-tntsearch-driver": "^11.3",
        "laravel/legacy-factories": "^1.1"
    },
    "autoload": {
        "classmap": [
            "app/Console/Commands/FakerDataStore.php",
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers/bootstrap_helper.php",
            "app/helpers/config_helper.php",
            "app/helpers/env_helper.php",
            "app/helpers/site_helper.php",
            "shims/Module_controller.php",
            "shims/View.php",
            "shims/Model.php",
            "shims/Capsule.php"
        ],
        "psr-4": {
            "munkireport\\models\\": "app/models/",
            "munkireport\\processors\\": "app/processors/",
            "munkireport\\builders\\": "database/builders/",
            "munkireport\\lib\\": "app/lib/munkireport",
            "Tests\\": "tests/",
            "App\\": "app/",
            "MR\\": "MR/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "composer.local.json"
            ],
            "replace": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php please package:discover --ansi",
            "MR\\Kiss\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php please key:generate --ansi"
        ]
    },
    "suggest": {
        "ext-ldap": "Needed to support Active Directory/LDAP Authentication"
    }
}
